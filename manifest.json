{
	"name": "Cloudburst",
	"version": "1.0",
	"description": "A clean and modern note taking extension.",
	"permissions": ["storage"],
	"background": {
		"page": "background.html",
		"persistent": false
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"options_page": "settings.html",
	"manifest_version": 2
}